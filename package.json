{
  "name": "flight-booking-api",
  "version": "1.0.0",
  "description": "A flight booking management API service",
  "main": "./dist/index.js",
  "scripts": {
    "format": "npx prettier ./src/**/* --write",
    "build": "npx esbuild src/main.ts --bundle --minify --platform=node --target=node16.20 --outfile=./dist/index.js",
    "start": "node ./dist/index.js",
    "prepare": "npx husky install",
    "generate:key": "node src/utils/generate-private-key.js"
  },
  "keywords": [
    "node",
    "express",
    "javascript",
    "typescript",
    "prisma",
    "flight",
    "booking",
    "api",
    "rest",
    "restful"
  ],
  "author": "Waryen <jonathangomand@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-sslify": "^1.2.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.15",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-sslify": "^1.2.0",
    "jsonwebtoken": "^9.0.0"
  },
  "volta": {
    "node": "16.20.0"
  },
  "engines": {
    "node": "16.20.0"
  },
  "lint-staged": {
    "*.{js,ts}": "prettier --write"
  }
}
